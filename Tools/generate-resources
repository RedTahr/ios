#!/usr/bin/env bash

DIRECTORY=$(dirname $0)
OUTPUT="$DIRECTORY/../Ringly/Ringly/Resources.swift"

echo "// Generated by tools/generate-resources, do not modify directly!" > "$OUTPUT"
swiftgen images -t swift3 "$DIRECTORY/../Ringly/Ringly/Images.xcassets" >> "$OUTPUT"
swiftgen strings -t swift3 "$DIRECTORY/../Ringly/Ringly/Localizable.strings" >> "$OUTPUT"
